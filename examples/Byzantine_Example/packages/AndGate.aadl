package AndGate
public
with Base_Types;
	system AndGate
		features
			in1 : in data port Base_Types::Boolean;
			in2 : in data port Base_Types::Boolean;
			in3 : in data port Base_Types::Boolean;
			in4 : in data port Base_Types::Boolean;
			
			and_out : out data port Base_Types::Boolean;
		
		annex agree {** 
			
			guarantee "And gate behavior." :
				and_out = (in1 and in2 and in3 and in4);
			
		**};
	end AndGate;
end AndGate;