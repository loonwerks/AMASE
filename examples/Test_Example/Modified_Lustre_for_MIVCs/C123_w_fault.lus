  __GUARANTEE1 : bool;
  __fault__independently__active__C1__C1__fault_1 : bool;
  __fault__independently__active__C2__C2__fault_1 : bool;
let
  --%MAIN;
  __ASSUME0 = ((in1 > 0) and (in1 < 10));

  __ASSUME1 = ((in2 > 0) and (in2 < 10));

  __ASSUME__HIST = (C3____ASSUME__HIST and (C2____ASSUME__HIST and (C1____ASSUME__HIST and true)));

  __GUARANTEE0 = ((out3 > 0) and (out3 < 20));

  __GUARANTEE1 = (out3 = (in1 + in2));
  
  __fault__independently__active__C1__C1__fault_1 = false;
  __fault__independently__active__C2__C2__fault_1 = false;

  assert __ASSUME0;

  assert __ASSUME1;

  assert (time = C1__time);

  assert _TOP__C1(C1____ASSUME__HIST, C1____ASSUME0, C1__in1, C1__time, C1____fault__nominal__out1, C1__fault__trigger__C1__fault_1, C1__out1);

  assert (C1____ASSUME__HIST = __HIST((C1____ASSUME0 and true)));

  assert (time = C2__time);

  assert _TOP__C2(C2____ASSUME__HIST, C2____ASSUME0, C2__in2, C2__time, C2____fault__nominal__out2, C2__fault__trigger__C2__fault_1, C2__out2);

  assert (C2____ASSUME__HIST = __HIST((C2____ASSUME0 and true)));

  assert (time = C3__time);

  assert _TOP__C3(C3____ASSUME__HIST, C3__in13, C3__in23, C3__time, C3__out3);

  assert (C3____ASSUME__HIST = __HIST(true));

  assert (in1 = C1__in1);

  assert (in2 = C2__in2);

  assert (C3__out3 = out3);

  assert (C1__out1 = C3__in13);

  assert (C2__out2 = C3__in23);

  assert (__fault__independently__active__C1__C1__fault_1 = (__fault__event__C1__C1__fault_1 -> (__fault__event__C1__C1__fault_1 or (pre __fault__independently__active__C1__C1__fault_1))));

  assert (__fault__dependently__active__C1__C1__fault_1 = (false -> (false or (pre __fault__dependently__active__C1__C1__fault_1))));

  assert (C1__fault__trigger__C1__fault_1 = (__fault__independently__active__C1__C1__fault_1 or __fault__dependently__active__C1__C1__fault_1));

  assert (__fault__independently__active__C2__C2__fault_1 = (__fault__event__C2__C2__fault_1 -> (__fault__event__C2__C2__fault_1 or (pre __fault__independently__active__C2__C2__fault_1))));

  assert (__fault__dependently__active__C2__C2__fault_1 = (false -> (false or (pre __fault__dependently__active__C2__C2__fault_1))));

  assert (C2__fault__trigger__C2__fault_1 = (__fault__independently__active__C2__C2__fault_1 or __fault__dependently__active__C2__C2__fault_1));

  assert (__fault__global_count = ((if __fault__independently__active__C1__C1__fault_1 then 1 else 0) + (if __fault__independently__active__C2__C2__fault_1 then 1 else 0)));

  --assert (__fault__global_count <= 0);

  assert true;

  assert (((time = 0.0) -> (time > (pre time))) and (true -> (time = ((pre time) + (time - (pre time))))));

  --%PROPERTY C1____ASSUME0;
  --%PROPERTY C2____ASSUME0;
  --%PROPERTY __ASSUME__HIST;
